[lniu001@hammer cs100hw2]$ make
g++ -g -Wall -Werror -ansi -pedantic src/rshell.cpp -o bin/rshell
[lniu001@hammer cs100hw2]$ bin/rshell
lniu001@hammer.cs.ucr.edu$ test -e README.md && echo a
a
lniu001@hammer.cs.ucr.edu$ test -e README.md || echo a
lniu001@hammer.cs.ucr.edu$ test -e README.md && echo "path exists"
"path exists"
lniu001@hammer.cs.ucr.edu$ test -f README.md && echo "path exists"
"path exists"
lniu001@hammer.cs.ucr.edu$ test -f README.md || echo "path exists"
lniu001@hammer.cs.ucr.edu$ test -d src && echo "path exists"
"path exists"
lniu001@hammer.cs.ucr.edu$ test -d src || echo "path exists"
lniu001@hammer.cs.ucr.edu$ test -e no && echo "path exists"
lniu001@hammer.cs.ucr.edu$ test -e no || echo "path exists"
"path exists"
lniu001@hammer.cs.ucr.edu$ ls
bceli001  bin  Makefile  README.md  src  test
lniu001@hammer.cs.ucr.edu$ exit
[lniu001@hammer cs100hw2]$
